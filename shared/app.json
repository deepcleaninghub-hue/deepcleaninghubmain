{
  "expo": {
    "name": "Deep Cleaning Hub",
    "slug": "deepclean-mobile-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcons/appstore.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/AppIcons/appstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#2d5016"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "9",
      "bundleIdentifier": "com.deepcleanhub.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "tel",
          "telprompt",
          "whatsapp"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2d5016"
      },
      "icon": "./assets/AppIcons/playstore.png",
      "package": "com.deepcleanhub.mobile",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 9
    },
    "web": {
      "favicon": "./assets/AppIcons/appstore.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/AppIcons/appstore.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "be614d02-dcd2-448c-a527-8872e01f4ae5"
      }
    }
  }
}
